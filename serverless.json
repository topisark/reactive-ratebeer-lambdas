{
  "service": "reactive-ratebeer-lambdas",
  "provider": {
    "name": "aws",
    "runtime": "nodejs6.10",
    "region": "eu-west-1"
  },
  "functions": {
    "beers": {
      "handler": "lambdas/beers.handler",
      "events": [
        {
          "http": {
            "path": "beers",
            "method": "get"
          }
        }
      ]
    }
  }
}